generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_UNPOOLED")
  directUrl = env("DATABASE_URL_UNPOOLED")
}


model Course {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  price       Float
  image       String?
  videoUrl    String?
  createdAt   DateTime @default(now())
}

